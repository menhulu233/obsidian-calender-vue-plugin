{
  "name": "periodic-notes-calendar",
  "version": "1.0.0",
  "description": "A calendar plugin for Obsidian with year/quarter/month/week views, supporting customizable periodic notes",
  "main": "dist/main.js",
  "scripts": {
    "dev": "vite build --watch --mode development",
    "build": "vite build --mode production && npm run copy-assets",
    "copy-assets": "node -e \"const fs=require('fs');if(!fs.existsSync('dist'))fs.mkdirSync('dist');fs.copyFileSync('manifest.json','dist/manifest.json')\"",
    "typecheck": "vue-tsc --noEmit"
  },
  "keywords": [
    "obsidian",
    "calendar",
    "vue3"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.0"
  },
  "dependencies": {
    "moment": "^2.29.4",
    "obsidian": "latest",
    "obsidian-daily-notes-interface": "^0.9.4",
    "vue": "^3.3.0",
    "pinia": "^2.1.0"
  }
}
